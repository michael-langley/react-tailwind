{
  "name": "tailwind-cra",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.19",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "classnames": "^2.2.6",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:styles": "postcss src/styles/tailwind.css -o src/styles/styles.css",
    "watch:styles": "postcss src/styles/tailwind.css -o src/styles/styles.css --watch",
    "svgr:icons": "svgr -d src/icons/ assets/icons/",
    "svgr:patterns": "svgr -d src/patterns/ assets/patterns/",
    "prestart": "npm run build:styles",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/preset-create-react-app": "^1.5.0",
    "@storybook/react": "^5.2.8",
    "@svgr/cli": "^5.0.1",
    "@types/classnames": "^2.2.9",
    "@types/ramda": "^0.26.39",
    "autoprefixer": "^9.7.3",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "tailwindcss": "^1.1.4"
  }
}
